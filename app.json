      "package": "com.anonymous.birthmealapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDsadPAbIgJa8JNvCGwPWVBY0NUn5MKcoE"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/Burger-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "9312b05c-245a-4e8c-a9cd-097c0ebe345a"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Burger-logo2.png",
          "color": "#A9005F"
        }
